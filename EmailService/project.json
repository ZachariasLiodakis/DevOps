{
  "name": "email-service",
  "root": "EmailService",
  "sourceRoot": "EmailService/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./mvnw clean package",
        "cwd": "EmailService"
      },
      "outputs": ["EmailService/target"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./mvnw test",
        "cwd": "EmailService"
      }
    }
  }
}