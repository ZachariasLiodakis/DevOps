{
  "name": "akinita-api",
  "root": "Akinita/Akinita",
  "sourceRoot": "Akinita/Akinita/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./mvnw clean package",
        "cwd": "Akinita/Akinita"
      },
      "outputs": ["Akinita/Akinita/target"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./mvnw test",
        "cwd": "Akinita/Akinita"
      }
    }
  }
}